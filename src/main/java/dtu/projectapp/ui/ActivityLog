package dtu.projectapp.ui;

import java.util.Optional;

import javafx.scene.control.TextInputDialog;

public class ActivityLog extends InputDialog {
    public EnterLoggedHours() {
        resetDialog();
    }

    @Override
    public void resetDialog() {
        super.dialog = new TextInputDialog("");
        super.dialog.setTitle("ACTIVITY_NAME");
        super.dialog.setHeaderText("Enter the amount of time spent on [ACTIVITY_NAME]");
    }
}